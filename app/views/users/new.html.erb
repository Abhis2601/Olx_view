 
<h1>OLX</h1>


<%= form_with url:'/users',method: :post do |form| %>
  <%= form.label :first_name %>
  <%= form.text_field :first_name %> <br>
  <%= form.label :last_name %>
  <%= form.text_field :last_name %> <br>
  <%= form.label :email %>
  <%= form.email_field :email %> <br>
  <%= form.label :password %>
  <%= form.password_field :password %> <br> 
  <%= form.label :mobile_no %>
  <%= form.number_field :mobile_no %> <br>
  <%= form.submit "SignUp" %>
<% end %>

<%= button_to "Signin", signin_users_path, method: :get %>